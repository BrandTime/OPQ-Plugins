                    )
                end
                if MsgType == "AtMsg" then
                    --Data {"Content":"@Kar98k skjjkssjkjs","UserID":123456789,"tips":"[AT消息]"}
                    Api.Api_SendMsg(
                        CurrentQQ,
                        {
                            toUser = GroupID,
                            sendToType = 2,
                            sendMsgType = "TextMsg",
                            groupid = 0,
                            content = string.format(
                                "捕获到成员 %d\n═══════════\n撤回了一条at消息\n消息内容为：\n%s",
                                extData.UserID,
