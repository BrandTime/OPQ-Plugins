                        }
                    )
                end

                if MsgType == "ReplayMsg" then
                    --Data {"MsgSeq":3536,"ReplayContent":"11 @Mac","SrcContent":"...","UserID":123123,"tips":"[回复]"}
                    --log.info("sql %s", json.decode(Data).url)
                    Api.Api_SendMsg(
                        CurrentQQ,
                        {
                            toUser = GroupID,
                            sendToType = 2,
                            sendMsgType = "TextMsg",
