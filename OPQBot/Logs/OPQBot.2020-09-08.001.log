                        CurrentQQ,
                        {
                            toUser = GroupID,
                            sendToType = 2,
                            sendMsgType = "PicMsg",
                            --发送图文消息
                            content = string.format(
                                "捕获到成员 %d\n═══════════\n撤回了一条图片消息\n消息内容为：[PICFLAG]%s",
                                extData.UserID,
                                jData.Content
                            ),
                            --通过宏[PICFLAG]改变图文顺序  改为现文字后图片
