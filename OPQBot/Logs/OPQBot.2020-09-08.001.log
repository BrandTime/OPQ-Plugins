    ok, err = c:connect({host = mysqlhost, port = 3306, database = mysqldb, user = mysqluser, password = mysqlpass})
    --建立连接
    if ok then
        sqlstr =
            string.format(
            [[INSERT INTO msgcache (GroupID, MsgSeq, MsgRandom,MsgType,Data,MsgTime)VALUES (%d,%d,%d,'%s','%s',%d)]],
            data.FromGroupId,
            data.MsgSeq,
            data.MsgRandom,
