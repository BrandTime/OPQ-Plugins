--url编码转换
function escape(w)
	pattern="[^%w%d%._%-%* ]"
	s=string.gsub(w,pattern,function(c)
		local c=string.format("%%%02X",string.byte(c))
		return c
	end)
	s=string.gsub(s," ","+")
	return s
end

--读取数据函数
function Read(url)
    file = io.open(url, "r")
	if (file == nil) then
