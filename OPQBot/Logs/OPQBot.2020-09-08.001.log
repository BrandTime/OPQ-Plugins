                    --log.info("sql %s", json.decode(Data).url)
                    Api.Api_SendMsg(
                        CurrentQQ,
                        {
                            toUser = GroupID,
                            sendToType = 2,
                            sendMsgType = "VoiceMsg",
                            --发送群语音消息
                            groupid = 0,
                            content = "",
                            atUser = 0,
